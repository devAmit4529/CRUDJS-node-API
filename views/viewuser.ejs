
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet">
</head>

<body>
    

    <table class="table table-hover table-dark">
        <thead>
            <tr>
                <th scope="col">ID</th>
                <th scope="col">Firstname</th>
                <th scope="col">lastname</th>
                <th scope="col">Email</th>
                <th scope="col">Update</th>
                <th scope="col">Delete</th>
            </tr>
        </thead>
        <tbody>

            <% data.map(function(result) { %>


                <tr>
                    <td>
                        <%= result._id %>

                    </td>
                    <td>
                        <%= result.first_name %>

                    </td>
                    <td>
                        <%= result.last_name %>
                    </td>
                    <td>
                        <%= result.email %>
                    </td>
                    <td>
                        <a class="btn btn-success " href="edit/<%= result._id %>"
                        onclick=""> edit</button>
                    </td>
                    <td>
                        <a class="btn btn-success " href="delete/<%= result._id %>"
                            onclick="alert('Deleted')">delete</a>
                    </td>
                </tr>
                <% }); %>
        </tbody>
    </table>
    <a class="btn btn-success " href="./addUser"
        onclick="">Add Alien</a>
    <a class="btn btn-success " href="./loginuser"
        onclick="">LogIn</a>


</body>

</html>
